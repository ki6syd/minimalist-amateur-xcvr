{
    "build": {
      "arduino":{
        "ldscript": "esp32s2_out.ld"
      },
      "core": "esp32",
      "extra_flags": [
        "-DARDUINO_ESP32S2_DEV",
        "-DARDUINO_USB_CDC_ON_BOOT=1"
      ],
      "f_cpu": "240000000L",
      "f_flash": "80000000L",
      "flash_mode": "qio",
      "mcu": "esp32s3",
      "variant": "esp32s3"
    },
    "connectivity": [
      "wifi"
    ],
    "debug": {
      "openocd_target": "esp32s3.cfg"
    },
    "frameworks": [
      "arduino",
      "espidf"
    ],
    "name": "ESP32-S3 MAX-4B",
    "upload": {
        "flash_size": "4MB",
        "maximum_ram_size": 327680,
        "maximum_size": 4194304,
        "speed": 115200,
        "use_1200bps_touch": true,
        "wait_for_upload_port": true,
        "require_upload_port": true
      },
    "vendor": "DIY",
    "url": "http://www.ki6syd.com"
